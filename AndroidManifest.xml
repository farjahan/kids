<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.farjahan.mystore"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.farjahan.mystore.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.farjahan.mystore.GameActivity" />
        <activity android:name="com.farjahan.mystore.GameOver" />
        <activity android:name="com.farjahan.mystore.Scores" />
        <activity android:name="com.farjahan.mystore.Mydatabase" 
              android:label="@string/app_name" >
            </activity>
        
    </application>
 

</manifest>
